{
  "name": "@tillhub/react-table-wrapper",
  "private": false,
  "version": "1.2.1",
  "description": "A wrapper over react-table, with several abstraction out of the box",
  "author": "erezsob",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tillhub/react-table-wrapper.git"
  },
  "main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "build": "rimraf dist && rollup -c",
    "start": "rollup -c -w",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build",
    "storybook": "start-storybook -p 6006",
    "semantic-release": "semantic-release",
    "commit": "git-cz",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "prop-types": "^15.5.4",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^3.0.0",
    "@semantic-release/git": "^7.0.3",
    "@semantic-release/github": "^5.0.4",
    "@semantic-release/npm": "^5.0.4",
    "@storybook/addon-actions": "^4.0.0-alpha.24",
    "@storybook/addon-links": "^4.0.0-alpha.24",
    "@storybook/addons": "^4.0.0-alpha.24",
    "@storybook/react": "^4.0.0-alpha.24",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.5",
    "babel-loader": "^7.1.5",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "classnames": "^2.2.6",
    "commitizen": "^2.10.1",
    "conventional-changelog-cli": "^2.0.5",
    "cross-env": "^5.1.4",
    "cz-conventional-changelog": "^2.1.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.4.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.0.1",
    "eslint-config-prettier": "^3.1.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-react": "^6.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-standard": "^3.1.0",
    "gh-pages": "^2.0.1",
    "memoize-one": "^4.0.2",
    "prettier": "^1.14.3",
    "prettier-eslint": "^8.8.2",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-scripts": "^1.1.4",
    "react-spinners": "^0.4.5",
    "react-svg-loader": "^2.1.0",
    "react-table": "^6.8.6",
    "react-test-renderer": "^16.4.2",
    "react-testing-library": "^5.2.0",
    "react-toastify": "^4.4.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.64.1",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^1.6.2",
    "rollup-plugin-url": "^1.4.0",
    "semantic-release": "^15.9.12",
    "storybook": "^1.0.0",
    "styled-components": "^3.4.9",
    "validate-commit-msg": "^2.14.0"
  },
  "files": [
    "dist",
    "package.json",
    "package-lock.json",
    "src/**/*.{js?x}",
    "CHANGELOG.md"
  ],
  "dependencies": {},
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "types": "conventional-commit-types",
      "helpMessage": "Use \"npm run commit\" instead, we use conventional-changelog format :) (https://github.com/commitizen/cz-cli)"
    }
  },
  "release": {
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ],
    "prepare": [
      "@semantic-release/npm",
      "@semantic-release/changelog",
      {
        "path": "@semantic-release/git",
        "assets": [
          "dist",
          "package.json",
          "package-lock.json",
          "src",
          "CHANGELOG.md"
        ],
        "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
      }
    ],
    "publish": [
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "bugs": {
    "url": "https://github.com/tillhub/react-table-wrapper/issues"
  },
  "homepage": "https://github.com/tillhub/react-table-wrapper#readme"
}
